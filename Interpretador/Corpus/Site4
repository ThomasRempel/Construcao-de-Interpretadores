sentenca4 = ''
arraySentencas4 = []


url4 = "https://www.coursera.org/specializations/natural-language-processing"


dadosPagina4 = requests.get(url4)
html4 = BeautifulSoup(dadosPagina4.text, 'html.parser')

#Se desejar printar o código html da url apenas descomentar a linha a seguir, eu achei desnecessário
#print(soup)

for sentenca4 in html4.find_all("p"):
    arraySentencas4.append(sentenca4.get_text())
    print(sentenca4.get_text())
    

# Print do Array
print('\n', 'Lista de sentenças: ', arraySentencas4)
