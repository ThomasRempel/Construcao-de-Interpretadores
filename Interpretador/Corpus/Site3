sentenca3 = ''
arraySentencas3 = []


url3 = "https://www.datarobot.com/blog/what-is-natural-language-processing-introduction-to-nlp/"


dadosPagina3 = requests.get(url3)
html3 = BeautifulSoup(dadosPagina3.text, 'html.parser')

#Se desejar printar o código html da url apenas descomentar a linha a seguir, eu achei desnecessário
#print(soup)

for sentenca3 in html3.find_all("p"):
    arraySentencas3.append(sentenca3.get_text())
    print(sentenca3.get_text())
    

# Print do Array
print('\n', 'Lista de sentenças: ', arraySentencas3)
